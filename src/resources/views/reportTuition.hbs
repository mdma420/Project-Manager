<div class="main__Report-tuition">
    <div class="table__Report-tuition">
        <h2>Báo cáo học phí</h2>
        <div class="show__Report-tuition">
            <table>
        <thead>
          <tr>
            <th id="th-nameTuition">Name Tuition</th>          
            <th id="th-period">Tuition Period</th>
            <th id="th-totalTuition">Total Tuition</th>
          </tr>
        </thead>
        <tbody>
          {{#each reportTuition}}
            <tr>
              <td id="td-nameTuition">{{this.nameTT}}</td>
              <td id="td-period">{{moment this.tuitionPeriod1 format ="DD MMM YYYY"}} - {{moment this.tuitionPeriod2 format ="DD MMM YYYY"}}</td>
              <td id="td-totalTuition">{{this.totalTT}}.vnd</td>
            </tr>
          {{/each}}
        </tbody>
      </table>
      </div>
    </div>
</div>