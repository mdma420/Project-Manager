    <div>
        <label for="b">b</label>
        <input type="number" name="b" id="b">
    </div>
    <div>
        <button type="submit">Create</button>
    </div>
    </form>
    <div>
        <button><a href="/teacher">Back</a></button>
    </div>
</div>