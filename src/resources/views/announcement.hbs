<div class="main__announcement">
    <div class="table_announcement-header">
        <h2>List Announcement</h2>
    </div>
<div class="table_announcement-body">
    <table>
        <thead>
            <tr>
                <th>Code</th>
                <th>Name</th>
                <th>Email</th>
                <th>Subject</th>
                <th>Sent Date</th>
            </tr>
        </thead>
        <tbody>
            {{#each email}}
            <tr>
                <td>{{this.codeStudent}}</td>
                <td>{{this.nameStudent}}</td>
                <td>{{this.emailStudent}}</td>
                <td>{{this.subject}}</td>
                <td>{{moment this.createdAt format ="DD MMM YYYY"}}</td>
            </tr>
            {{/each}}
            {{#each emailTeacher}}
            <tr>
                <td>{{this.codeTeacher}}</td>
                <td>{{this.nameTeacher}}</td>
                <td>{{this.emailTeacher}}</td>
                <td>{{this.subject}}</td>
                <td>{{moment this.createdAt format ="DD MMM YYYY"}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div> 
</div>