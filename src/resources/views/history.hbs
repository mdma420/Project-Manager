<div class="main__Historytuition">
    <div class="table__History-show">
       <div class="table__History-header">
        <h2>Lịch sử thu học phí</h2>
        </div>
        <div class="table__History-body">
        <table>
            <thead>
                <tr>
                    <th>code invoice</th>
                    <th>Ngày thu</th>
                    <th>Người nộp</th>
                    <th>Học Phí</th>
                    <th>Cách đóng</th>
                </tr>
            </thead>
            <tbody>
                {{#each invoice}}
                <tr>
                    <td>{{this._id}}</td>
                    <td>{{moment this.createdAt format ="DD MMM YYYY"}}</td>
                    <td>{{this.name}}</td>
                    <td>{{this.tuition}}</td>
                    <td>{{this.method}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
        </div>
    </div>
    </div>
