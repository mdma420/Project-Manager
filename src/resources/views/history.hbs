<div class="main__History">
    <div class="table__History">
       <div class="table__History-invoice">
        <h2>Lịch sử thu học phí</h2>
        <div class="show__History">
        <table>
            <thead>
                <tr>
                    <th>Mã phiếu thu</th>
                    <th>Ngày thu</th>
                    <th>Người nộp</th>
                    <th>Học Phí</th>
                    <th>Thực thu</th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                {{#each invoice}}
                <tr>
                    <td>{{this._id}}</td>
                    <td>{{moment this.createdAt format ="DD MMM YYYY"}}</td>
                    <td>{{this.name}}</td>
                    <td>{{this.fee}}</td>
                    <td>{{this.fee}}</td>
                    <td>xem phiếu thu</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
        </div>
       </div>
       <div class="table__History-noti">
        <h2>Lịch sử gửi thông báo</h2>
        <div class="show__History">
        <table>
            <thead>
                <tr>
                    <th>Tiêu đề</th>
                    <th>Nội dung</th>
                    <th>Ngày gửi</th>
                    <th>Giờ gửi</th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                {{#each email}}
                <tr>
                    <td>{{this.subject}}</td>
                    <td>{{this.html}}</td>
                    <td>{{moment this.createdAt format ="DD MMM YYYY"}}</td>
                    <td></td>
                    <td>xem thông báo</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
        </div>
       </div>
    </div>
</div>